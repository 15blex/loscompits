		"svelte": "^5.43.7",
		"svelte-check": "^4.3.4",
		"svelte-persisted-store": "^0.12.0",
		"typescript": "^5.9.3",
		"vite": "^7.2.2",
		"wuchale": "^0.18.5"
	}
